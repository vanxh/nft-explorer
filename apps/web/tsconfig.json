{
  "extends": "@nft-explorer/tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/lib/*": ["src/lib/*"],
      "@/public/*": ["public/*"],
      "@/env/*": ["env/*"]
    }
  }
}
