{
  "extends": "@nft-explorer/tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/lib/*": ["src/lib/*"],
      "@/env/*": ["src/env/*"],
      "@/styles/*": ["src/styles/*"],
      "@/public/*": ["public/*"]
    }
  }
}
